{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../build"
  },
  "package": {
    "productName": "eftify",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "dialog": {
        "open": true
      }
    },
    "updater": {
      "active": true,
      "endpoints": [
        "https://eftify.soun-tech.net/{{target}}/{{arch}}/{{current_version}}"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEY2ODlERkU2NTNDMDMzNkYKUldSdk04QlQ1dCtKOWgydDRSZGM5bzBrbjhpUklFbTJLTVhKREt4a01sZTE3OEFvVXJiSUs2aGUK",
      "windows": {
        "installMode": "passive"
      }
    },
    "windows": [],
    "systemTray": {
      "iconPath": "./icons/32x32.png"
    },
    "security": {
      "csp": null
    },
    "bundle": {
      "windows": {
        "wix": {
          "language": ["en-US", "ja-JP"]
        }
      },
      "active": true,
      "targets": "all",
      "identifier": "net.soun-tech.eftify",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}
